%h1 Editing application

= error_messages_for :application

- form_for(@application) do |f|
  %p
    %b Name
    = f.text_field :name 
  %p
    %b Wait wav
    = f.text_field :wait_wav 
  %p
    %b App human
    = f.text_field :app_human 
  %p
    %b App machine
    = f.text_field :app_machine 
    
  %p= f.submit "Update"
  
= link_to 'Show', application_path(@application)
= link_to 'Back', applications_path

