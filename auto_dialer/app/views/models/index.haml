%h1 Listing models

%table
  %tr
    %th Test
    %th Test2

- for model in @models
  %tr
    %td=h model.test
    %td=h model.test2
  
    %td= link_to 'Show', model_path(model)
    %td= link_to 'Edit', edit_model_path(model)
    %td= link_to 'Destroy', model_path(model), :confirm => 'Are you sure?', :method => :delete, :confirm => "Are you sure?", :method => :delete
    
= link_to 'New model', :action=>"new"